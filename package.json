{
  "productName": "SikhiToTheMax",
  "name": "sttm-desktop",
  "version": "4.2.1",
  "description": "The SikhiToTheMax desktop app",
  "main": "www/main/app.js",
  "scripts": {
    "precommit": "npm test",
    "test": "htmlhint --config .htmlhintrc www && eslint packaging/ src/js && stylelint \"src/scss/*.scss\" --syntax scss --fix",
    "start": "cross-env NODE_ENV=development electron .",
    "clean": "rimraf www/assets/js/*.js www/main www/shared",
    "build": "npm run clean && webpack && cp -R src/js/main www/main && cp -R src/js/shared www/shared && npm run build-css",
    "pack:mac": "npm run build && build --mac",
    "pack:win": "npm run build && build --win --x64",
    "pack:win32": "npm run build && build --win --ia32",
    "pack:linux": "npm run build && build --linux --x64",
    "upload:mac": "node packaging/upload-mac.js",
    "upload:win": "node packaging/upload-win.js",
    "upload:win32": "node packaging/upload-win32.js",
    "dist:mac": "npm run pack:mac && npm run upload:mac",
    "dist:win": "npm run pack:win && npm run upload:win",
    "dist:win32": "npm run pack:win32 && npm run upload:win32",
    "build-css": "run-p build-css:*",
    "build-css:main": "node-sass --output-style compressed --include-path src/scss src/scss/styles.scss www/assets/css/bundle.css",
    "build-css:obs": "node-sass --output-style compressed --include-path src/scss/obs src/scss/obs/styles.scss www/obs/bundle.css",
    "watch": "nodemon --watch src -x \"npm run build\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:khalisfoundation/sttm-desktop.git"
  },
  "author": "Khalis, Inc. <techsupport@khalisfoundation.org>",
  "license": "KNC-GSL",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "aws-sdk": "^2.28.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^3.3.0",
    "cross-env": "^3.1.4",
    "electron": "^1.6.2",
    "electron-builder": "^20.4.0",
    "electron-publisher-s3": "^16.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "htmlhint": "^0.10.1",
    "husky": "^0.13.3",
    "markdown-loader": "^4.0.0",
    "marked": "^0.3.19",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "spectron": "^3.6.1",
    "ssh2": "^0.5.4",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.4.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.0.8",
    "electron-chromecast": "^1.1.0",
    "electron-log": "^2.0.2",
    "electron-redux": "^1.3.1",
    "electron-updater": "^2.21.0",
    "express": "^4.15.2",
    "extract-zip": "^1.6.7",
    "get-json": "^1.0.0",
    "hyperscript": "^2.0.2",
    "ip": "^1.1.5",
    "is-online": "^7.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "moment": "^2.22.1",
    "noty": "^3.2.0-beta",
    "openport": "0.0.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "realm": "^2.18.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "scroll": "^2.0.1",
    "socket.io": "^2.1.1",
    "sqlite3": "^4.0.3",
    "tingle.js": "^0.13.2",
    "tippy.js": "^2.5.2",
    "update": "^0.7.4"
  },
  "build": {
    "appId": "org.khalisfoundation.sttm",
    "copyright": "Copyright Â© 2018 Khalis Foundation , SikhiToTheMax Trademark SHARE Charity, UK\n",
    "files": [
      "node_modules",
      "www",
      "package.json",
      "LICENSE"
    ],
    "extraResources": [
      "data.db"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "builds"
    },
    "publish": {
      "provider": "s3",
      "bucket": "sttm-releases",
      "path": "${os}-${arch}",
      "region": "us-west-2"
    },
    "dmg": {
      "icon": "STTM-DMG.icns"
    },
    "mac": {
      "category": "public.app-category.reference",
      "icon": "assets/STTM3.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "nsis": {
      "artifactName": "${productName}Setup-${version}.${ext}",
      "deleteAppDataOnUninstall": true,
      "perMachine": true
    },
    "win": {
      "icon": "assets/STTME.ico",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": "deb",
      "icon": "assets"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
